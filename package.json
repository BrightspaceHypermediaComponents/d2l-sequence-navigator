{
  "name": "d2l-sequence-navigator",
  "version": "0.1.4",
  "description": "",
  "private": true,
  "scripts": {
    "postinstall": "polymer install",
    "precommit": "lint-staged",
    "lint:html": "eslint --fix *.html demo/*.html components/*.html test/*.html utility/*.html gulpfile.js",
    "lint": "npm run lint:html",
    "test": "polymer test --skip-plugin sauce",
    "test-p": "polymer test --skip-plugin sauce -p",
    "locales": "gulp build-locales && npm run gen:localize",
    "prestart": "npm run locales && polymer analyze > analysis.json",
    "gen:localize": "plop localize",
    "start": "polymer serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BrightspaceUI/d2l-sequence-navigator.git"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "chai-dom": "^1.8.0",
    "eslint": "^4.2.0",
    "eslint-config-brightspace": "^0.3.1",
    "eslint-plugin-html": "^3.1.0",
    "gulp": "^3.9.1",
    "gulp-merge-json": "^1.3.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "plop": "^2.0.0",
    "polymer-cli": "^1.3.1",
    "polymer-siren-test-helpers": "^1.0.0",
    "sinon": "^5.0.7"
  },
  "dependencies": {
    "bower-art-resolver": "^2.0.9",
    "npm": "^6.0.0"
  },
  "lint-staged": {
    "*.html": [
      "eslint",
      "git add"
    ]
  }
}
